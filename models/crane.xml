<mujoco> <!-- Simulator options -->
    <option timestep="0.01" iterations="1" ls_iterations="5" integrator="RK4" gravity="0 0 0 " /> <!--
    Defaults -->
    <default> <!-- No contact, everything is grey-->
        <geom rgba="0.5 0.5 0.5 1" conaffinity="0" /> <!-- Note that joint damping has a big impact
        on sim stability, esp. with the tendon -->
        <joint damping="1e-4" />
    </default> <!-- The robot -->
    <worldbody> <!-- 大车机构 -->
        <body name="AxisX_Frame" pos="0 0 20">
            <joint name="AxisX" type="slide" axis="1 0 0" />
            <geom type="box" size="0.5 0.5 0.05" mass="1" /> <!--小车机构 -->
            <body name="AxisY_Frame" pos="0 0 0">
                <joint name="AxisY" type="slide" axis="0 1 0" />
                <geom type="box" size="0.5 0.5 0.05" mass="1" rgba="0.92 0.25 0.25 1" /> <!-- The
                site for attaching the cable -->
                <site name="playload_top" pos="0 0 -0.1" />
            </body>
        </body> <!--
        The payload -->
        <body name="payload" pos="0.0 0.0 5.0">
            <joint type="free" />
            <geom type="capsule" size=".3 0.3" rgba="0.99 0.99 0.99 1.0" /> <!-- Site to attach the
            top of the payload -->
            <site name="payload_end" pos="0 0 0.1" />
        </body>
    </worldbody> <!-- The cable
    holding the payload -->
    <tendon>
        <spatial name="cable" damping="0.0">
            <site site="playload_top" />
            <site site="payload_end" />
        </spatial>
    </tendon> <!-- 小车执行器 -->
    <actuator>
        <position name="posX" joint="AxisX" ctrlrange="-10 10" kp="10" dampratio="1.0" />
        <position name="posY" joint="AxisY" ctrlrange="-10 10" kp="10" dampratio="1.0" />
        <position name="posZ" tendon="cable" kp="10" dampratio="1.0" forcerange="-10 0"
            ctrlrange="-12 0" />
    </actuator>
</mujoco>